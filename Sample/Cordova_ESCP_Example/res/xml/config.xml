<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jamjalee.blackbox" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <allow-intent href="market:*" />
    <name>CITIZEN ESCP Printer</name>
    <description>
        Cordova/Phonegap plugins for CITIZEN CMP Printers.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
	<platform name="android"> 
		<source-file src="com.apache.cordova.plugins.citizen.cmp/CalculatorPlugin.java" target-dir="src/com/apache/cordova/plugins/citizen/cmp" /> 

		<config-file target="res/xml/config.xml" parent="/*"> 
			<feature name="CitizenEscpPrinter" > 
				<param name="android-package" value="com.apache.cordova.plugins.citizen.cmp.CitizenEscpPrinter"/> 
			</feature> 
		</config-file> 

		<config-file target="AndroidManifest.xml" parent="/manifest">
		</config-file> 
	</platform> 
</widget>
